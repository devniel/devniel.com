{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --watch",
    "dev": "webpack-dev-server --open --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "build:static": "webpack --config webpack.static.js",
    "hack": "webpack --config webpack.server.js && node build/bundle.js",
    "lint": "eslint src",
    "lint:staged": "eslint",
    "prettier": "prettier --write \"**/*.{scss,css,js,md}\"",
    "prettier:staged": "prettier --write",
    "prettier:diff": "prettier --list-different \"**/*.{scss,css,js,md}\""
  },
  "author": "",
  "license": "ISC",
