  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "yarn prettier:staged",
        "yarn lint:staged",
        "git add"
      ],
      "*.{css,md,scss}": [
        "yarn prettier:staged",
        "git add"
      ]
    },
    "ignore": [
      "public/*.js",
      "static/*.js"
    ]
  }
